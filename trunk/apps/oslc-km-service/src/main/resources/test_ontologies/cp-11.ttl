<http://www.reusecompany.com/ontology/cp-1> rdf:type owl:Ontology ;
                                            
                                            rdfs:label "This is an ontology"^^xsd:string .


#################################################################
#
#    Classes
#
#################################################################


###  http://www.reusecompany.com/ontology/cp-1#Bicycle

:Bicycle rdf:type owl:Class ;
         
         rdfs:subClassOf :Vehicle .



###  http://www.reusecompany.com/ontology/cp-1#Car

:Car rdf:type owl:Class ;
     
     rdfs:subClassOf :Vehicle ;
     
	rdfs:subClassOf :Vehicle ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :has ;
                       owl:onClass :Wheel ;
                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                     ] ;
					 
	 rdfs:subClassOf :Vehicle ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :has ;
                       owl:someValuesFrom :Wheel
                     ] ;
					 
     owl:disjointUnionOf ( :Bicycle
                           :Truck
                         ) .



###  http://www.reusecompany.com/ontology/cp-1#Truck

:Truck rdf:type owl:Class ;
       
       rdfs:subClassOf :Vehicle .



###  http://www.reusecompany.com/ontology/cp-1#Vehicle

:Vehicle rdf:type owl:Class ;
         
         rdfs:label "Vehicle"^^xsd:string .



###  http://www.reusecompany.com/ontology/cp-1#Wheel

:Wheel rdf:type owl:Class .





#################################################################
#
#    Individuals
#
#################################################################


###  http://www.reusecompany.com/ontology/cp-1#MiPeugeot

:MiPeugeot rdf:type :Car ,
                    owl:NamedIndividual ;
           
           owl:sameAs :Peugeot .



###  http://www.reusecompany.com/ontology/cp-1#Peugeot

:Peugeot rdf:type :Car ,
                  owl:NamedIndividual .





#################################################################
#
#    General axioms
#
#################################################################


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Bicycle
                :Car
                :Truck
              )
] .




