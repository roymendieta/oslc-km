###  http://www.reusecompany.com/ontology/cp-1#has

:has rdf:type owl:ObjectProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.reusecompany.com/ontology/cp-1#Car

:Car rdf:type owl:Class ;
     
     rdfs:subClassOf :Vehicle ,
                     [ rdf:type owl:Class ;
                       owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                              owl:onProperty :has ;
                                              owl:onClass :Pedal ;
                                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger
                                            ]
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :has ;
                                              owl:onClass :Wheel ;
                                              owl:qualifiedCardinality "4"^^xsd:nonNegativeInteger
                                            ]
                                          )
                     ] ;
     
     owl:disjointWith :Truck .



###  http://www.reusecompany.com/ontology/cp-1#Pedal

:Pedal rdf:type owl:Class .



###  http://www.reusecompany.com/ontology/cp-1#Truck

:Truck rdf:type owl:Class ;
       
       rdfs:subClassOf :Vehicle .



###  http://www.reusecompany.com/ontology/cp-1#Vehicle

:Vehicle rdf:type owl:Class ;
         
         rdfs:label "Vehicle"^^xsd:string .



###  http://www.reusecompany.com/ontology/cp-1#Wheel

:Wheel rdf:type owl:Class .





#################################################################
#
#    Individuals
#
#################################################################


###  http://www.reusecompany.com/ontology/cp-1#P1

:P1 rdf:type :Pedal ,
             owl:NamedIndividual .



###  http://www.reusecompany.com/ontology/cp-1#Peugeot

:Peugeot rdf:type :Car ,
                  owl:NamedIndividual ;
         
         :has :P1 ,
              :W1 ,
              :W2 ,
              :W3 ,
              :W4 .



###  http://www.reusecompany.com/ontology/cp-1#W1

:W1 rdf:type :Wheel ,
             owl:NamedIndividual .



###  http://www.reusecompany.com/ontology/cp-1#W2

:W2 rdf:type :Wheel ,
             owl:NamedIndividual .



###  http://www.reusecompany.com/ontology/cp-1#W3

:W3 rdf:type :Wheel ,
             owl:NamedIndividual .



###  http://www.reusecompany.com/ontology/cp-1#W4

:W4 rdf:type :Wheel ,
             owl:NamedIndividual .





#################################################################
#
#    General axioms
#
#################################################################


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( :W1
                        :W2
                        :W3
                        :W4
